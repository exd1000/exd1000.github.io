      50% { opacity: 0.5; filter: blur(4px); }
      100% { opacity: 1; filter: blur(0); transform: translateY(0); }
    }
    .fade-section { opacity: 0; transform: translateY(30px); filter: blur(8px); transition: opacity 0.4s ease, transform 0.4s ease, filter 0.4s ease; }
    .fade-section.visible { animation: fogReveal 0.6s ease forwards; }
  
  
    html, body {
    height: 100%;
    width: 100%;
    margin: 0;
    overflow-y: auto;   
    overflow-x: hidden;
  }

